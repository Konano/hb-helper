{
  "manifest_version": 3,
  "name": "华宝条件单自动延期",
  "version": "0.0.1",
  "description": "华宝智投条件单自动延期",
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "images/16x16.png",
      "48": "images/48x48.png",
      "128": "images/128x128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://*.touker.com/*"],
      "js": ["src/content.js"]
    }
  ],
  "permissions": ["activeTab", "tabs", "storage", "http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
